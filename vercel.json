{
  "rewrites": [
    {
      "source": "/",
      "destination": "api/index"
    },
    {
      "source": "/api/round/:round",
      "destination": "/api/round"
    },
    {
      "source": "/api/image/:mime_sub_type/:file_id",
      "destination": "/api/image"
    }
  ],
  "installCommand": "npm ci --omit=dev",
  "buildCommand": "node scripts/build.mjs",
  "github": {
    "silent": true
  }
}
